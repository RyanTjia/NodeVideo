doctype html
html
	//Uses a different .pug file to fill out the header
	include layouts/dashhead.pug

	body
		include layouts/dashnav.pug
		
		each value in videos
			div(class='row')
				div(class='col-lg-10 mx-auto')
					div(class='row')
						div(class='col-lg-5')
							iframe(width='320' height='240' src=`${value['Link']}`)
						div(class='col-lg-7')
							div(class='row')
								div(class='col-lg-12')
									h1 #{value['Title']}
							div(class='row')
								div(class='col-lg-12') #{value['Desc']}